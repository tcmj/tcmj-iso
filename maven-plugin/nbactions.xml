<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-generate_enum</actionName>
            <displayName>generate_enum</displayName>
            <goals>
                <goal>com.tcmj.plugins:pug-enum-maven-plugin:1.2.8.1-SNAPSHOT:generate-enum</goal>
            </goals>
            <properties>
                <com.tcmj.pug.enums.classname>com.tcmj.html.ClassNameHtmlEnum</com.tcmj.pug.enums.classname>
                <com.tcmj.pug.enums.url>https://en.wikipedia.org/wiki/ISO_3166-1</com.tcmj.pug.enums.url>
                <com.tcmj.pug.enums.cssselector>[title=Afghanistan]</com.tcmj.pug.enums.cssselector>
                <com.tcmj.pug.enums.constantcolumn>1</com.tcmj.pug.enums.constantcolumn>
                <com.tcmj.pug.enums.subdatacolumns>2,3,4</com.tcmj.pug.enums.subdatacolumns>
                <com.tcmj.pug.enums.exporter>com.tcmj.iso.exporter.impl.InMemoryCompilingExporter</com.tcmj.pug.enums.exporter>
                <com.tcmj.pug.enums.subfieldnames>alpha2Code,alpha3Code,numericCode</com.tcmj.pug.enums.subfieldnames>
                <com.tcmj.pug.enums.classjavadoc>This is a dynamically generated java enum class.,Data has been fetched from ${com.tcmj.pug.enums.url}</com.tcmj.pug.enums.classjavadoc>
            </properties>
        </action>
        
    </actions>
